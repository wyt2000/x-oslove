{
	"name": "learn",
	"displayName": "learn",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:learn.helloWorld",
		"onCommand:editor.action.clipboardCopyAction",
		"onCommand:learn.undo",
		"onLanguage:plaintext"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "learn.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "learn.undo",
				"title": "undo"
			}
		],
		"keybindings": [
			{
				"command": "learn.undo",
				"key": "ctrl+z",
				"mac": "cmd",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
